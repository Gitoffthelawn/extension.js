{
  "name": "Extension.js DevTools",
  "description": "Extension.js built-in developer tools for instance ID __INSTANCE_ID__",
  "version": "1.0.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "type": "module"
  },
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "permissions": ["management", "tabs", "storage"]
}
